{
  "$schema": "https://docs.molt.bot/schema/openclaw.json",
  "meta": {
    "lastTouchedVersion": "2026.2.1",
    "lastTouchedAt": "2026-02-06T00:00:00.000Z"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "auth": {
    "profiles": {
      "openai:default": {
        "provider": "openai",
        "mode": "api_key"
      }
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "pairing",
      "groupPolicy": "allowlist",
      "streamMode": "partial",
      "botToken": "YOUR_TELEGRAM_BOT_TOKEN"
    }
  },
  "agents": {
    "defaults": {
      "workspace": "~/.openclaw/workspace",
      "model": {
        "primary": "openai/gpt-5.2",
        "fallbacks": ["openai/gpt-4o-mini"]
      },
      "sandbox": {
        "mode": "non-main",
        "workspaceAccess": "rw",
        "scope": "agent",
        "docker": {
          "image": "openclaw-sandbox:latest",
          "network": "bridge"
        }
      },
      "tools": {
        "profile": "full",
        "exec": {
          "host": "sandbox",
          "security": "allowlist",
          "ask": "off"
        }
      }
    },
    "list": [
      {
        "id": "main",
        "default": true,
        "name": "Main",
        "workspace": "~/.openclaw/workspace",
        "sandbox": { "mode": "off" },
        "tools": { "profile": "full" }
      },
      {
        "id": "dev",
        "name": "Dev",
        "workspace": "~/.openclaw/workspace",
        "sandbox": {
          "mode": "all",
          "workspaceAccess": "rw",
          "scope": "agent",
          "docker": {
            "image": "openclaw-sandbox:latest",
            "network": "bridge"
          }
        },
        "tools": {
          "profile": "coding",
          "exec": { "host": "sandbox", "security": "allowlist", "ask": "off" }
        }
      },
      {
        "id": "research",
        "name": "Research",
        "workspace": "~/.openclaw/workspace",
        "sandbox": {
          "mode": "all",
          "workspaceAccess": "rw",
          "scope": "agent",
          "docker": {
            "image": "openclaw-sandbox:latest",
            "network": "bridge"
          }
        },
        "tools": {
          "profile": "minimal",
          "alsoAllow": ["group:web", "browser"],
          "deny": ["exec"]
        }
      }
    ]
  }
}
